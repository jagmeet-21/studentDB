@model IEnumerable<atwal_Quiz2.Models.property>

@{
    ViewBag.Title = "Index";
}

<h2>Property</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.propName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.propName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.propNo }) |
            @Html.ActionLink("Details", "Details", new { id=item.propNo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.propNo })
        </td>
    </tr>
}

</table>
